{
  "extends": [
    "prettier",
    "prettier/react"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc": "off"
  },
  "globals": {
    "document": true,
    "getComputedStyle": true
  },
  "env": {
    "node": true,
    "es6": true
  }
}
